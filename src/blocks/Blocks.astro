---
import Hero from "./Hero.astro";
import Carousel from "../components/Carousel.astro"
// Import other block components

const { data } = Astro.props;
---

{data.map(block => {
  switch (block.blockType) {
    case 'hero':
      return <Hero {...block} />;
    case 'carousel':
      return <Carousel {...block} />;
    // // Add cases for other block types
    default:
      return null;
  }
})}